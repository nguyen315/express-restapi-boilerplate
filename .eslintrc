{
  "parser": "@babel/eslint-parser",
  "extends": ["prettier"],
  "plugins": ["prettier"],
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true,
    "browser": false
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": false,
      "ecmaVersion": "es6"
    },
    "sourceType": "module"
  },
  "globals": {
    "strapi": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "error"
  }
}
